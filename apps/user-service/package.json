{
  "name": "@ai-job-portal/user-service",
  "version": "1.0.0",
  "description": "User Profile Management Microservice for AI Job Portal",
  "scripts": {
    "build": "nest build && pnpm run copy-proto",
    "copy-proto": "node ./scripts/copy-proto.js",
    "predev": "nest build",
    "dev": "nest start --watch --entryFile apps/user-service/src/main",
    "start": "node dist/apps/user-service/src/main",
    "start:debug": "nest start --debug --watch --entryFile apps/user-service/src/main",
    "start:prod": "node dist/apps/user-service/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ai-job-portal/common": "workspace:*",
    "@ai-job-portal/database": "workspace:*",
    "@ai-job-portal/types": "workspace:*",
    "@aws-sdk/client-s3": "^3.521.0",
    "@aws-sdk/s3-request-presigner": "^3.521.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^6.12.0",
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.8.0",
    "drizzle-orm": "^0.29.3",
    "fastify": "^4.25.2",
    "file-type": "^16.5.4",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "mammoth": "^1.6.0",
    "mime-types": "^2.1.35",
    "nanoid": "^3.3.7",
    "openai": "^4.24.1",
    "pdf-parse": "^1.1.1",
    "postgres": "^3.4.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/jest": "^29.5.11",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.0",
    "@types/pdf-parse": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3"
  }
}
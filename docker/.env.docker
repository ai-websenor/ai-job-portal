# Docker Compose Environment Variables
# Copy to docker/.env and modify as needed

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ai_job_portal_dev

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# LocalStack (AWS emulator)
AWS_ENDPOINT_URL=http://localstack:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=ap-south-1

# S3
S3_BUCKET=ai-job-portal-dev-uploads

# SQS
SQS_NOTIFICATION_QUEUE_URL=http://localstack:4566/000000000000/ai-job-portal-dev-notifications

# SES
SES_FROM_EMAIL=noreply@aijobportal.com
SES_FROM_NAME=AI Job Portal
